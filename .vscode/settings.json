{
	"eslint.enable": true,
	"eslint.codeActionsOnSave.mode": "all",
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true
		}
	},
	"[typescriptreact]": {
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true
		}
	},
	"[json][jsonc][jsonl]": {
		"editor.defaultFormatter": "vscode.json-language-features",
		"editor.formatOnSave": true
	}
}